<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pane" prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.teafactory.controller.HomeFormController">
    <children>
        <ImageView fitHeight="768.0" fitWidth="1366.0">
            <image>
                <Image url="@../assets/Home.jpg" />
            </image>
        </ImageView>
        <Label fx:id="lblFactoryName" layoutX="35.0" layoutY="28.0" prefHeight="157.0" prefWidth="720.0" text="Hadungoda Tea Factory ">
            <font>
                <Font name="Bodoni MT" size="64.0" />
            </font>
            <textFill>
                <LinearGradient endX="1.0" endY="1.0">
                    <stops>
                        <Stop color="#2e4a05" />
                        <Stop color="#0e9765" offset="1.0" />
                    </stops>
                </LinearGradient>
            </textFill>
        </Label>
        <Button fx:id="btnLogin" layoutX="1158.0" layoutY="681.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="35.0" prefWidth="135.0" style="-fx-background-color: #02C39A;" text="Login" textFill="#901717">
            <font>
                <Font size="18.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
